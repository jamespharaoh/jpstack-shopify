<record
	name="shopifyMetafield"
	type="major">

	<dao-interfaces>

		<interface
			package="wbs.integrations.shopify.model"
			name="ShopifyDaoMethods">

			<parameter value="wbs.integrations.shopify.model.ShopifyMetafieldRec"/>

		</interface>

	</dao-interfaces>

	<object-helper-interfaces>

		<interface
			package="wbs.integrations.shopify.model"
			name="ShopifyObjectHelper">

			<parameter value="wbs.integrations.shopify.model.ShopifyMetafieldRec"/>

		</interface>

	</object-helper-interfaces>

	<record-interfaces>

		<interface
			package="wbs.integrations.shopify.model"
			name="ShopifyRecord">

			<parameter value="wbs.integrations.shopify.model.ShopifyMetafieldRec"/>

		</interface>

	</record-interfaces>

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			name="account"
			type="shopifyAccount"/>

		<identity-integer-field
			name="shopifyId"/>

		<!-- details -->

		<deleted-field/>

		<!-- data -->

		<timestamp-field
			name="createdAt"
			column-type="postgresql"
			nullable="yes"/>

		<string-field
			name="description"
			nullable="yes"/>

		<string-field
			name="key"
			nullable="yes"/>

		<string-field
			name="namespace"
			nullable="yes"/>

		<integer-field
			name="ownerId"
			nullable="yes"/>

		<enum-field
			name="ownerResource"
			type="shopifyMetafieldOwnerResource"
			nullable="yes"/>

		<timestamp-field
			name="updatedAt"
			column-type="postgresql"
			nullable="yes"/>

		<string-field
			name="value"
			nullable="yes"/>

		<string-field
			name="valueType"
			nullable="yes"/>

	</fields>

	<shopify-synchronisation>

		<scalar-fields>

			<field name="created-at"/>
			<field name="description"/>
			<field name="key"/>
			<field name="namespace"/>
			<field name="owner-id"/>
			<field name="owner-resource"/>
			<field name="updated-at"/>
			<field name="value"/>
			<field name="value-type"/>

		</scalar-fields>

	</shopify-synchronisation>

</record>